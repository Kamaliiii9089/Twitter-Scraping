{
  "name": "twitter-scraper-ui",
  "version": "1.0.0",
  "description": "React interface for custom Twitter scraping",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "setup": "npm install && cd client && npm install",
    "client:install": "cd client && npm install",
    "server:install": "npm install"
  },
  "keywords": [
    "twitter",
    "scraper",
    "react",
    "express",
    "puppeteer"
  ],
  "author": "Twitter Scraper Team",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.21.2",
    "node-fetch": "^2.7.0",
    "puppeteer": "^21.11.0",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10"
  }
}
